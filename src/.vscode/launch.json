{
    "version": "0.2.0",
    "configurations": [
         {
             "name": ".NET Core Launch (web)",
             "type": "coreclr",
             "request": "launch",
             "preLaunchTask": "build",
             "program": "${workspaceFolder}/Api.Application/bin/Debug/netcoreapp3.1/application.dll",
             "args": [],
             "cwd": "${workspaceFolder}\\Api.Application",
             "stopAtEntry": false,
             "serverReadyAction": {
                 "action": "openExternally",
                 "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
             },
             "env": {
                 "ASPNETCORE_ENVIRONMENT": "Development",
                 "DB_CONNECTION_STRING" : "Persist Security Info=True;Server=.\\SQLSERVER2019;Initial Catalog=dbapi;MultipleActiveResultSets=true;User ID=sa;Password=mudar@123",
                 "DATABASE" : "MYSQL",
                 "MIGRATION" : "APLICAR",
                 "Audience": "ExemploAudience",
                 "Issuer": "ExemploIssuer",
                 "Seconds": "28800"
             },
             "sourceFileMap": {
                 "/Views": "${workspaceFolder}/Views"
             }
         },
         {
             "name": ".NET Core Attach",
             "type": "coreclr",
             "request": "attach",
             "processId": "${command:pickProcess}"
         }
     ]
 }